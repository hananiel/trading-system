{
  "name": "trading-system",
  "version": "1.0.0",
  "description": "Temporal-based staged trading workflow",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "worker": "ts-node src/worker.ts",
    "run-workflow": "ts-node src/runWorkflow.ts",
    "demo": "ts-node src/runWorkflow.ts",
    "demo-parallel": "ts-node src/runWorkflow.ts",
    "demo-sequential": "ts-node src/runWorkflow.ts --sequential --delay 1000",
    "demo-custom": "ts-node src/runWorkflow.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo 'No linting configured'",
    "typecheck": "tsc --noEmit",
    "regression": "./regression-test.sh",
    "clean": "rm -f trade-decisions.csv"
  },
  "dependencies": {
    "@temporalio/activity": "^1.8.0",
    "@temporalio/client": "^1.8.0",
    "@temporalio/common": "^1.8.0",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "yahoo-finance": "^0.3.8"
  },
  "devDependencies": {
    "@temporalio/testing": "^1.8.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "temporal",
    "trading",
    "workflow"
  ],
